        console.log(typeof(b));
        console.log(typeof(c));
        console.log(typeof(t));
        console.log(typeof(f));

        //Operators (+, -, *, /, %)
        console.log("Operators");
        var num=0;
        num++; //num--
        ++num;  //--num
        console.log(num);
        var num=21;
        num=25;
        console.log(num);
        var a=10;
        var b=5;
        console.log(a+b);     //addition
        console.log(a-b);     //sunbtraction
        console.log(a*b);     //multiplication
        console.log(a/b);    //quotient
        console.log(a%b);   //remaindeer
        console.log(a**2);   //power
        console.log(a<=b);   // greater than equal
        console.log(a>=b);   //less that equal
        console.log(a==b);   //equal to
        console.log(a===b);  //thrible equalto
        console.log(a!=b);   //not equal
        
        //conditional 
        //(&&==> both conditions must be true)
        //(||==> atleast one condition be true )
        console.log("conditional")
        var age=21;
        age==21 && age==20;
        console.log(age==21 && age==20); //false
        console.log(age!==21 || age==21);  //true
       
        //loops
        console.log("loops")
        //for
